disksim_logorg org0 {
  Addressing mode = Parts,
  Distribution scheme = Asis,
  Redundancy scheme = Noredun,
  devices = [ disk0 .. disk15 ],
  Number of copies = 2,
  Stripe unit  =  32,
  Synch writes for safety =  1,
